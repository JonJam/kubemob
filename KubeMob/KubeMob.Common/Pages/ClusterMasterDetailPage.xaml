<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="KubeMob.Common.Pages.ClusterMasterDetailPage"
             xmlns:pages="clr-namespace:KubeMob.Common.Pages">

    <MasterDetailPage.Master>
        <pages:ClusterMasterPage/>
    </MasterDetailPage.Master>

    <MasterDetailPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <pages:ClusterOverviewPage />
            </x:Arguments>
        </NavigationPage>
    </MasterDetailPage.Detail>
</MasterDetailPage>